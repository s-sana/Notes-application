$("ul.errors").html("")
<% if @note.errors.any? %>
    <% @note.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".notes-index").html("<%= escape_javascript(render 'index') %>");
    $("#notes-form").modal("hide")
<% end %>
